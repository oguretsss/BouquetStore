@model List<string>
@{
    int i = 0;
}
<div class="row text-center">
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <h3>Мы в Инстаграме</h3>
    </div>
    <div class="col-md-2"></div>
</div>
<div class="row">
    @foreach (var pic in Model)
    {
        <div class="col-md-4 thumb @(i > 3 ? "hidden-sm" : "")">
            <a href="https://www.instagram.com/fruit_is_goood/" target="_blank">
                <div class="warp"><img class="img-thumbnail center-block" src="@pic" width="300" height="300" alt="Fruit Is Good в Instagram"></div>
            </a>
        </div>
        i++;

    }
</div>
